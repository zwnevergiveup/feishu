<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_conversation">

    <fragment
        android:id="@+id/navigation_conversation"
        android:name="com.example.feishu.ui.conversation.ConversationFragment"
        android:label="@string/conversation"
        tools:layout="@layout/fragment_conversation" />

    <fragment
        android:id="@+id/navigation_compact"
        android:name="com.example.feishu.ui.compact.CompactFragment"
        android:label="@string/compact"
        tools:layout="@layout/fragment_compact" />

    <fragment
        android:id="@+id/navigation_mine"
        android:name="com.example.feishu.ui.mine.MineFragment"
        android:label="@string/mine"
        tools:layout="@layout/fragment_mine" />




</navigation>
